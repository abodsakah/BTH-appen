{
  "expo": {
    "name": "BTH app",
    "slug": "BthApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#09718C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#09718C"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.abodsakka.bthstudent",
      "config": {
        "googleMaps": {
          "apiKey": "[api key]"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#09718C"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e5b88002-cadf-4bd1-92de-f6fee8587b9b"
      }
    }
  }
}
